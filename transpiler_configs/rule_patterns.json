{
  "patterns": {
    "core_obligation": {
      "patterns": ["pay_rent", "grant_use"],
      "description": "Core lease obligations",
      "priority": 1
    },
    "maintenance_obligation": {
      "patterns": ["maintain"],
      "description": "System maintenance obligations",
      "priority": 2
    },
    "return_obligation": {
      "patterns": ["return"],
      "description": "System return obligations",
      "priority": 2
    },
    "generation_obligation": {
      "patterns": ["permit", "representation"],
      "description": "Energy generation permit obligations",
      "priority": 3
    },
    "interconnection_obligation": {
      "patterns": ["interconnect"],
      "description": "System interconnection obligations",
      "priority": 3
    },
    "sales_obligation": {
      "patterns": ["sell", "surplus"],
      "description": "Energy sales obligations",
      "priority": 3
    },
    "rights_obligation": {
      "patterns": ["guarantee", "peaceful"],
      "description": "Rights and guarantees",
      "priority": 4
    },
    "delivery_obligation": {
      "patterns": ["deliver"],
      "description": "System delivery obligations",
      "priority": 4
    }
  },
  "grouping": {
    "generation_activities": ["generation_obligation", "interconnection_obligation", "sales_obligation"],
    "core_activities": ["core_obligation", "maintenance_obligation", "return_obligation"],
    "rights_activities": ["rights_obligation", "delivery_obligation"]
  }
}
